{
    "name": "ais-forge",
    "version": "0.0.3-beta",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "tsx watch src/index.ts",
        "build": "tsup src/index.ts --format esm --platform node --out-dir dist",
        "lint": "biome check .",
        "lint:fix": "biome check --write .",
        "typecheck": "tsc --noEmit",
        "test": "echo \"Error: no test specified\" && exit 0",
        "db:generate": "pnpm drizzle-kit generate",
        "db:migrate": "pnpm drizzle-kit migrate",
        "db:migrate:test": "cross-env DRIZZLE_ENV=test drizzle-kit migrate",
        "db:push": "pnpm drizzle-kit push",
        "db:studio": "pnpm drizzle-kit studio"
    },
    "keywords": [],
    "author": "SkyZon - Jean-Pierre Dupuis",
    "license": "Apache-2.0",
    "packageManager": "pnpm@10.27.0",
    "devDependencies": {
        "@biomejs/biome": "2.3.11",
        "@types/node": "^25.0.3",
        "@types/pg": "^8.16.0",
        "cross-env": "^10.1.0",
        "drizzle-kit": "^0.31.8",
        "tsup": "^8.5.1",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "@noble/post-quantum": "^0.5.4",
        "@node-rs/argon2": "^2.0.2",
        "dotenv": "^17.2.3",
        "drizzle-orm": "^0.45.1",
        "fastify": "^5.6.2",
        "ioredis": "^5.9.1",
        "jose": "^6.1.3",
        "pg": "^8.16.3",
        "uuid": "^13.0.0",
        "zod": "^4.3.5"
    }
